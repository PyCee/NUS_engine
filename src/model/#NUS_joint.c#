#include "NUS_joint.h"
#include <string.h>
#include <limits.h>
#include <stdlib.h>

NUS_joint nus_joint_build
(NUS_matrix inv_bind_pose, char const *string, int parent_index)
{
  NUS_joint joint;
  joint.inv_bind_pose = inv_bind_pose;
  strcpy(joint.name, string);
  joint.parent_index = parent;
  return joint;
}
void nus_joint_free(NUS_joint *p_joint)
{
}
